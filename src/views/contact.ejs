<!-- FORM Keep in touch -->
    <div class="container text-start" style="color: beige;">
        <form class="row g-2 mt-3 form-container"  action="https://api.web3forms.com/submit" method="POST">
            <input type="hidden" name="access_key" value="d948144a-eb9d-42b2-9279-3b6ddbe53d4e">
            <input type="hidden" name="from_name" value="192 Viagens">
            <input type="hidden" name="redirect" value="https://web3forms.com/success">
            <input type="checkbox" name="botcheck" id="" style="display: none;">

            <h4>Keep in touch:</h4>
            

                <!-- NAME -->
                <div class="col-6 form-floating" style="color: #213363;">
                    <input type="text" name="name" id="fname" class="form-control " placeholder="Name" required>
                    <label for="fname">Name</label>
                    
                </div>
                <!-- Last NAME -->
                <div class="col-6 form-floating" style="color: #213363;">
                    <input type="text" name="lastName" id="lname" class="form-control" placeholder="Last Name" required>
                    <label for="lname">Last Name</label>
                </div>
                <!-- Email -->
                <div class="row g-2">          
                    <div class="col form-floating" style="color: #213363;">
                        <input type="email" name="email" id="email" class="form-control" placeholder="Email" required>
                        <label for="email">Email</label>                   
                    </div>
                </div>
                <!-- types of trip -->
                <div class="row g-2 mb-4">          
                    <div class="col form-floating" style="color: #213363;">
                        <select class="form-select" aria-label="Default select example" id="typesoftrip">
                            <option value="0">Type of Trip</option>
                            <option value="1">National</option>
                            <option value="2">International</option>
                        </select>                 
                    </div>
                </div>
            
                
    
                <!-- Subject -->
                <div class="row">
                    <h5>Subject:</h5>
                    <input type="hidden" id='subject' name="Customer Standard Email" >
                        <div class="col col-md-4 form-check">
                            <input type="radio" name="subject" class="form-check-input" value="Consultancy" id="consultancy">
                            <label for="consultancy">Consultancy</label>
                        </div>
                        <div class="col col-md-4  form-check">
                            <input type="radio" name="subject" class="form-check-input" value="Personalized Routes" id="personalizedRoutes">
                            <label for="personalizedRoutes">Personalized Routes</label>
                        </div>
                        <div class="col col-md-4  form-check">
                            <input type="radio" name="subject" class="form-check-input" value='Other' id="other">
                            <label for="other">Other</label>
                        </div>
                    
                </div>
                
                <!-- Message -->
                <div class="row my-3">
                    <div class="col-10">
                        <textarea name="message"  cols="50" rows="5" class="form-control" placeholder="Write your message:" required></textarea>
                    </div>
                    
                    <div class="col-2 position-relative">
                        <button class="btn btn-lg btn-secondary position-absolute bottom-0 start-0" type="submit" id="submit">Submit</button>
                    </div>

                </div>
                
        </form>
